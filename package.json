{
  "name": "@ufly/sam",
  "version": "1.0.2",
  "description": "本地dev环境模拟器",
  "keywords": [
    "umi/dumi",
    "vite",
    "webpack",
    "nextjs",
    "vue-cli-service"
  ],
  "bin": {
    "sam": "bin/sam"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": "https://github.com/lamovv/sam.git",
  "license": "MIT",
  "dependencies": {
    "@js-util-fn/url": "^1.0.6",
    "anyproxy": "^4.1.3",
    "chalk": "4",
    "cosmiconfig": "^7.0.1",
    "detect-port": "^1.3.0",
    "figlet": "^1.5.2",
    "hostile": "^1.3.3",
    "ip": "^1.1.5",
    "log4js": "^6.4.3",
    "ora": "5",
    "shelljs": "^0.8.5",
    "v8-compile-cache": "^2.3.0",
    "yargs": "^14.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "esbuild": "^0.14.30",
    "eslint": "^8.12.0",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2"
  },
  "peerDependencies": {
    "koa": ">=2",
    "koa-static": ">=5"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "scripts": {
    "bootstrap": "husky install",
    "esbuild": "esbuild lib/*.js lib/**/*.js --outdir=dist --platform=node --minify-identifiers",
    "release": "standard-version -r patch",
    "postrelease": "git push --follow-tags origin master && cd dist && npm publish",
    "beta": "standard-version --prerelease beta",
    "postbeta": "git push --follow-tags origin master && cd dist && npm publish --tag beta",
    "alpha": "standard-version --skip.tag --prerelease alpha",
    "postalpha": "cd dist && npm publish --tag alpha",
    "postpublish": "rm -rf dist",
    "clear": "sam clear"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
