{
  "name": "@ufly/sam",
  "version": "1.0.8",
  "description": "本地dev环境模拟器",
  "keywords": [
    "umi/dumi",
    "vite",
    "webpack",
    "nextjs",
    "vue-cli-service"
  ],
  "bin": {
    "sam": "bin/sam"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamovv/sam.git"
  },
  "license": "MIT",
  "dependencies": {
    "@js-util-fn/url": "^1.0.6",
    "anyproxy": "^4.1.3",
    "chalk": "4",
    "co": "^4.6.0",
    "cosmiconfig": "^7.0.1",
    "detect-port": "^1.3.0",
    "figlet": "^1.5.2",
    "hostile": "^1.3.3",
    "ip": "^1.1.8",
    "log4js": "^6.5.2",
    "ora": "5",
    "shelljs": "^0.8.5",
    "v8-compile-cache": "^2.3.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/cz-commitlint": "^17.0.0",
    "commitizen": "^4.2.4",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0"
  },
  "peerDependencies": {
    "koa": ">=2",
    "koa-static": ">=5"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "postinstall": "husky install || true",
    "release": "(if [ -n \"$(git status -s)\" ];then\ngit add . -A\ncz\nfi) && standard-version -r patch",
    "beta": "standard-version --skip.tag --skip.commit -p beta",
    "postbeta": "cd dist && npm publish --tag beta",
    "alpha": "standard-version --skip.tag --skip.commit -p alpha",
    "postalpha": "cd dist && npm publish --tag alpha",
    "postpublish": "rm -rf dist",
    "clear": "sam clear"
  }
}
